<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxDialog" name="DeviceInfo">
		<title>Smartpen Device Information</title>
		<size>585,165</size>
		<minsize>585,165</minsize>
		<maxsize>585,165</maxsize>
		<id_arg>0</id_arg>
		<object class="wxFlexGridSizer" variable="mainSizer" member="no">
			<cols>3</cols>
			<object class="sizeritem">
				<object class="wxStaticBitmap" name="ID_STATICBITMAP1" variable="infoIcon" member="yes">
					<bitmap>/home/dylan/cpp/LibreScribe/res/dialog-information-128.png</bitmap>
					<size>128,128</size>
				</object>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="dialogSizer" member="no">
					<cols>1</cols>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT1" variable="deviceType" member="yes">
							<label>LightScribe Pulse(TM) Smartpen</label>
							<font>
								<size>13</size>
								<style>normal</style>
								<weight>bold</weight>
								<underlined>0</underlined>
								<family>swiss</family>
								<face>Sans</face>
							</font>
							<style>wxALIGN_LEFT</style>
						</object>
						<flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxFlexGridSizer" variable="informationSizer" member="no">
							<cols>2</cols>
							<rows>2</rows>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT3" variable="nameLabel" member="yes">
									<label>Name:</label>
								</object>
								<flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT4" variable="deviceName" member="yes">
									<label>Dylan Taylor&apos;s Smartpen</label>
								</object>
								<flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT5" variable="batteryLabel" member="yes">
									<label>Battery:</label>
								</object>
								<flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxGauge" name="ID_GAUGE1" variable="batteryGauge" member="yes">
									<value>50</value>
									<minsize>250,25</minsize>
									<maxsize>250,25</maxsize>
								</object>
								<flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT6" variable="storageLabel" member="yes">
									<label>Storage:</label>
								</object>
								<flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT2" variable="storageRemaining" member="yes">
									<label>1.55GB of 2.13GB remaining</label>
								</object>
								<flag>wxALL|wxALIGN_TOP|wxALIGN_BOTTOM</flag>
								<border>5</border>
								<option>1</option>
							</object>
						</object>
						<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
				<border>10</border>
				<option>1</option>
			</object>
		</object>
	</object>
</wxsmith>
